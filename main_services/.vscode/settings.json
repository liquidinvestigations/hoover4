{
    "python.defaultInterpreterPath": "${workspaceFolder}/processing/.venv/Scripts/python.exe",
    "mypy-type-checker.path": ["${workspaceFolder}/processing/.venv/Scripts/mypy"],
    "python.analysis.extraPaths": [
        "${workspaceFolder}/processing/.venv/Lib/site-packages",
        "${workspaceFolder}/processing"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "mypy-type-checker.cwd": "${workspaceFolder}/processing",
    "rust-analyzer.cargo.extraEnv": {
       "CARGO_HTTP_CHECK_REVOKE": "false",
    },
    "editor.rulers": [91],
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.shellIntegration.enabled": false,
    "files.autoSave": "afterDelay",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "explorer.confirmDelete": false,
    "editor.wordWrapColumn": 123,
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "deepIndent",
    "rust-analyzer.cargo.extraArgs": ["--target-dir", "target/rust-analyzer/"],
    "[toml]": {
        "editor.formatOnSave": true
    },

} 